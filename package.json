{
  "name": "cabal-desktop-mini",
  "version": "1.0.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "build": "bankai build && build",
    "dev": "bankai start index.js",
    "inspect": "bankai inspect index.js",
    "pack": "bankai build && build --dir",
    "start": "cross-env NODE_ENV=development electron main.js",
    "test": "standard && test-deps",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons"
  },
  "build": {
    "appId": "cabal-desktop-mini",
    "files": [
      "**/*"
    ],
    "win": {
      "target": [
        "squirrel"
      ]
    }
  },
  "dependencies": {
    "cabal-core": "^4.0.0",
    "choo": "^6.4.2",
    "choo-devtools": "^2.5.1",
    "collect-stream": "^1.2.1",
    "dat-encoding": "^5.0.1",
    "discovery-swarm": "^5.1.4",
    "electron-collection": "^1.2.0",
    "hypercore-crypto": "^1.0.0",
    "identicon.js": "^2.3.3",
    "js-yaml": "^3.12.1",
    "mkdirp": "^0.5.1",
    "random-access-memory": "^3.1.1",
    "tachyons": "^4.11.1"
  },
  "devDependencies": {
    "bankai": "^9.15.0",
    "cross-env": "^5.2.0",
    "electron": "^4.0.3",
    "electron-builder": "^20.38.2",
    "electron-builder-squirrel-windows": "^20.38.5",
    "standard": "^12.0.1"
  }
}
